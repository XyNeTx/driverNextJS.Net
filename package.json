{
  "name": "driver_tracking_nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run next\" \"npm run dotnet\"",
    "publish" : "concurrently \"npm run build\" \"npm run netpub\"",
